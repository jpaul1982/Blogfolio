<%- include ("./partials/header") %>
<h1>Specific Blog</h1>
<div class="card" style="width: 18rem;">
    <div class="card-body">
        <img src="<%=blog.source%>" class="card-img-top" alt="Blog post with image.">
        <h4> <%=blog.name%> </h4>
        <h4> <%=blog.year%> </h4>
        <h4> <%=blog.medium%> </h4>
        <h4> <%=blog.comments[0].text%> </h4>
        <p> <%=blog.description%> </p>

    </div>
  </div>


<%- include ("./partials/footer") %>