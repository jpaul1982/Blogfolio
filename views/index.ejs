<%- include ("./partials/header") %>
<div class="container">
  <div class=" container row">
    <div class="col-lg-12"> 
      <h2 id="index_subhead" style="text-align: center;">A catalogue of projects in art, design, and software development.</h2>
    </div>
  </div>
  <div class="container row">
   <div class="row" style="display:flex; flex-wrap:wrap;">
    <% blogs.forEach(function(blog) { %>
    <div class="item olive col-lg-3 col-md-6">
      <a href="/blog/<%= blog._id %>"><img src="<%=blog.source%>" class="index_image" alt="Blog post with image."></a>
      <i class="far fa-eye"></i>
    </div>
     <% }) %>
    </div>
  </div>
</div>

      

      
    
<%- include ("./partials/footer") %>