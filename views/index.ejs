<%- include ("./partials/header") %>
<div class="container">
  <div class=" container row">
    <div class="col-lg-12 col-md-12" > 
      <h2 id="index_subhead" style="text-align: center;"><i class="fab fa-pagelines"></i>Catalogue of Projects<i class="fab fa-pagelines"></i></h2>
    </div>
  </div>
  <div class="grid" style="display:flex; flex-wrap:wrap;">
    <% blogs.forEach(function(blog) { %>
    <div class="item">
      <a href="/blog/<%= blog._id %>"><img src="<%=blog.source%>" class="index_image" alt="Blog post with image."></a>
      <a style="color:black" href="/blog/<%= blog._id %>"><i class="far fa-eye"></i></a>
    </div>
     <% }) %>
    </div>
</div>


      

      
    
<%- include ("./partials/footer") %>